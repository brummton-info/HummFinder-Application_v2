            <!-- android:name="android.support.PARENT_ACTIVITY" -->
            <!-- android:value="hummfinderapp.alpha.MainActivity" /> -->
        </activity>
        <activity android:name=".matching.MatchingActivity">

            <!-- android:parentActivityName="hummfinderapp.alpha.MainActivity"> -->
            <!-- <meta-data -->
            <!-- android:name="android.support.PARENT_ACTIVITY" -->
            <!-- android:value="hummfinderapp.alpha.MainActivity" /> -->
        </activity>
        <activity android:name=".info.InfoActivity">

            <!-- android:parentActivityName="hummfinderapp.alpha.MainActivity" -->


            <!-- <meta-data -->
            <!-- android:name="android.support.PARENT_ACTIVITY" -->
            <!-- android:value="hummfinderapp.alpha.MainActivity" /> -->
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>